{
  "name": "rocketblend-desktop",
  "outputfilename": "rocketblend-desktop",
  "frontend:install": "npm install",
  "frontend:build": "npm run build --base=./",
  "frontend:dev:watcher": "npm run dev",
  "frontend:dev:serverUrl": "auto",
  "wailsjsdir": "./frontend/src/lib",
  "author": {
    "name": "LazerCube"
  },
  "obfuscated": true,
  "garbleargs": "-tiny -seed=myrandomseed",
  "info": {
    "companyName": "RocketBlend",
    "productVersion": "0.0.0",
    "copyright": "Copyright (c) 2024 RocketBlend",
    "comments": "RocketBlend Desktop Application",
    "fileAssociations": [
      {
        "ext": "blend",
        "name": "Blender",
        "description": "Blender Project File",
        "iconName": "appicon",
        "role": "Editor"
      }
    ]
  }
}
